{
    "run_config": {
        "target": "qemu",
        "xlspike": {
            "timeout": 480
        },
        "qemu": {
            "timeout": 240
        }
    },
    "parallel": "-j",
    "copy_objects": false,
    "build_target": "clean all",
    "build_config": {
        "SOC": "evalsoc",
        "DOWNLOAD": "ilm"
    },
    "checks": {
        "PASS": ["passed", "Passed"],
        "FAIL": ["MEPC", "failed", "Failed", "FAILURE", "ERROR", "test error apprears"]
    },
    "appdirs": [
        "DSP/Examples/RISCV",
        "DSP/Test"
    ],
    "build_configs": {
        "rv32imacp": { "CORE": "n300", "ARCH_EXT": "_xxldspn1x" },
        "rv32imacbp": { "CORE": "n300", "ARCH_EXT": "_zba_zbb_zbc_zbs_xxldspn1x" },
        "rv32imafcp": { "CORE": "n300f", "ARCH_EXT": "_xxldspn1x" },
        "rv32imafcbp": { "CORE": "n300f", "ARCH_EXT": "_zba_zbb_zbc_zbs_xxldspn1x" },
        "rv32imafcpv": { "CORE": "n300f", "ARCH_EXT": "_zve32f_xxldspn1x" },
        "rv32imafcbpv": { "CORE": "n300f", "ARCH_EXT": "_zve32f_zba_zbb_zbc_zbs_xxldspn1x" },
        "rv32imafdcp": { "CORE": "n300fd", "ARCH_EXT": "_xxldspn1x" },
        "rv32imafdcbp": { "CORE": "n300fd", "ARCH_EXT": "_zba_zbb_zbc_zbs_xxldspn1x" },
        "rv32imafdcpv": { "CORE": "n300fd", "ARCH_EXT": "_zve32f_xxldspn1x" },
        "rv32imafdcbpv": { "CORE": "n300fd", "ARCH_EXT": "_zve32f_zba_zbb_zbc_zbs_xxldspn1x" }
    }
}
