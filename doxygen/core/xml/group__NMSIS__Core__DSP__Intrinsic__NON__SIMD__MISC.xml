      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__NON__SIMD__MISC_1gac34824012cfe56f0a6fb9baf2fb1ba33" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
