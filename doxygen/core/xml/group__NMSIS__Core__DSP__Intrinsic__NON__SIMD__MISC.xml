      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__NON__SIMD__MISC_1ga5061e392780b3bea521580befe37556f" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
