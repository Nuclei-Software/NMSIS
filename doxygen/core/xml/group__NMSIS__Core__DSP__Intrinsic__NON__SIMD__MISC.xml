      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__NON__SIMD__MISC_1ga8260760ab7c146507ec4507862534332" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
