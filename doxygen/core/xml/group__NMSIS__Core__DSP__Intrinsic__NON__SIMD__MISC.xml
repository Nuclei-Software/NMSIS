      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__NON__SIMD__MISC_1ga1e5062cc77c31e898d6a85e7768a7b2d" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
