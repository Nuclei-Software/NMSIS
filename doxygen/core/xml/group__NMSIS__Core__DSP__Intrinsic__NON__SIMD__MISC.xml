      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__NON__SIMD__MISC_1ga3a4b869f2df434f7e9f3f35e79715c6c" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
