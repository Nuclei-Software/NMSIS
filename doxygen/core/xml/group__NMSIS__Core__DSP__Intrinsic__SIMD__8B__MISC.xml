      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__SIMD__8B__MISC_1gafed7823f6a501d256c3232bd8a1d85d1" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
