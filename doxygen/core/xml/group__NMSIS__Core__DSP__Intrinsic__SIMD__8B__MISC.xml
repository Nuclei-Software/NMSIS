      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__SIMD__8B__MISC_1gaa595d7e87e2f435c0ceb0e9d8c20ee97" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
