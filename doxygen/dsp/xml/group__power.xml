        <argsstring>(const q31_t *pSrc, uint32_t blockSize, q63_t *pResult)</argsstring>
        <name>riscv_power_q31</name>
        <param>
          <type>const q31_t *</type>
          <declname>pSrc</declname>
        </param>
        <param>
